#include <stdio.h>

int main()
{
    double nota1, nota2, media;

    printf("Digite a primeira nota: ");
    scanf("%lf",&nota1);
    printf("Digite segunda nota: ");
    scanf("%lf",&nota2);

    media = nota1 + nota2;

    printf("NOTA FINAL = %.1lf\n", media);

    if(media >= 60.00)
    {
        printf("APROVADO");
    }else
    {
        printf("REPROVADO");
    }

    return 0;
}
