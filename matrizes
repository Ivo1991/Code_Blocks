#include <stdio.h>

int main()
{
    int m, n, i, j;

    printf("Quantas linhas tera a matriz? ");
    scanf("%d",&m);
    printf("Quantas colunas tera a matriz? ");
    scanf("%d",&n);

    int mat[50][50];

    for(i = 0; i < m; i++)
    {
        for(j = 0; j < n; j++)
        {
            printf("Elemento [%d ,%d]: ", i, j);
            scanf("%d",&mat[i][j]);
        }
    }

    printf("\nMATRIZES DIGITADAS:\n");
    for(i = 0; i < m; i++)
    {
        for(j = 0; j < n; j++)
        {
            printf("%d\n", mat[i][j]);
        }
    }

    return 0;
}
